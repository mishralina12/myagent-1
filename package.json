{
  "name": "myagent-1",
  "version": "1.0.0",
  "description": "Synthetic Content Manager API - LinkedIn content automation platform",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx src/worker.ts",
    "worker:dev": "tsx watch src/worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "linkedin",
    "content-automation",
    "ai",
    "anthropic",
    "claude",
    "social-media"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.28.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "drizzle-orm": "^0.33.0",
    "postgres": "^3.4.0",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "passport-jwt": "^4.0.1",
    "@anthropic-ai/sdk": "^0.71.0",
    "bullmq": "^5.12.0",
    "ioredis": "^5.4.0",
    "zod": "^3.23.0",
    "dotenv": "^16.4.0",
    "dayjs": "^1.11.0",
    "uuid": "^10.0.0",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/bcrypt": "^5.0.2",
    "typescript": "^5.5.0",
    "tsx": "^4.16.0",
    "drizzle-kit": "^0.24.0",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0"
  }
}
